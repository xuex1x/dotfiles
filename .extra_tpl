
# Proxy Configuration
export HTTP_PROXY=http://proxy.com:port
export http_proxy=http://proxy.com:port
export HTTPS_PROXY=http://proxy.com:port
export https_proxy=http://proxy.com:port

# PYENV 
export PYENV_ROOT="$HOME/.pyenv"
[[ -d $PYENV_ROOT/bin ]] && export PATH="$PYENV_ROOT/bin:$PATH"
eval "$(pyenv init - bash)"
